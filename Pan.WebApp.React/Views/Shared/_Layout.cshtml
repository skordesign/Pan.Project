<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Pan.WebApp.React</title>
    <base href="~/" />
    <environment names="Staging,Production">    
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/kube/6.5.2/css/kube.min.css" asp-append-version="true"/>
    </environment>
    <environment names="Development">
          <link rel="stylesheet" href="~/css/kube.min.css" asp-append-version="true" />
    </environment>
     <link rel="stylesheet" href="~/dist/stylesheets/main.css" asp-append-version="true" />
</head>
<body>
    @RenderBody()

    <script src="~/dist/vendor.js" asp-append-version="true"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
